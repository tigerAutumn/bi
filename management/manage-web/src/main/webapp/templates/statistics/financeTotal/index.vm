        $("#hf_bank_acc").hide();
    }

    //恒丰账户
    function hfbankAcc() {
        var button_bao_foo = document.getElementById("button_bao_foo");
        button_bao_foo.style.backgroundColor = "#D3D3D3";

        var hfbank = document.getElementById("button_hfbank");
        hfbank.style.backgroundColor = "#0097ff";

		$("#baofoo_acc").hide();
		$("#hf_bank_acc").show();
    }
</script>