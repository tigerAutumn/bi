<script>
    function toAndroidPage(json) {
        javascript:coinharbour.toAndroidPage(json);
    }

    function go_login() {
        var json = '{"appActive":{"page":"b"}}';
        var client = document.getElementById("client").value;
        if(client == "ios") {
            toiOSPage(json);
        } else if(client == "android") {
            toAndroidPage(json);
        }
    }

    $('.go_list').on('click', function() {
        go_login();
    });
</script>
