    <div class="websiteWrapper"> 
      
      
      <!-- notification starts -->
      
      <!-- notification ends --> 
      
      <!-- page wrapper starts -->
	  <form action="$!env.getWebURL('/micro/user/login')" method="post" class="generalForm" id="generalForm">
			<input type="hidden" id="userNameReg" value="$!reg.get('nick')" />
			<input type="hidden" id="passwordReg" value="$!reg.get('password')" />
      <div class="pageWrapper generalPageWrapper" style="padding-top:0px;"> 
		##	<div class="tx3"><img src="$!env.getWebURL('')/resources/micro/images/tx2-92.png" width="80" height="80"></div>
		<div class="gm formFieldWrapper" style="border-bottom:5px ;border:1px solid #eee; border-radius:5px;width: 95%; margin-top:25px"><span><img src="$!env.getWebURL('')/resources/micro/images/zh.png" width="32" height="28">帐号</span> 
			<input type="text" name="nick" value="$!NICK" class="userNameField requiredField regExpField" id="userName" placeholder="请输入用户名/手机"/>
		</div>
		<div class="gm formFieldWrapper" style="border-bottom:5px ;border:1px solid #eee; border-radius:5px;width: 95%; margin-top:5px">
