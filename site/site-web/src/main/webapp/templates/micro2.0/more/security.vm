<script>
	var NowImg; //表示当前显示的div
    var MaxImg; //表示div的个数
    window.onload = function() {
    	NowImg = 1;
    	MaxImg = 10;
    	show()
    	theTimer = setInterval("show()", 4000); //设置定时器，显示下一个图片，10000表示每间隔10秒，可以改的。。。           	
    }
	function show() {
    	for(var i = 1; i <= MaxImg; i++) {
    		if(NowImg == i) {
    			$("#bg6_num" + NowImg).addClass("rel_number_copy").removeClass("rel_number")
    			$("#bg6_jiantou" + NowImg).addClass("ft_copy").siblings().removeClass("ft_copy")						
    			$("#bg6_ctxt" + NowImg).addClass("safe_wc_center_show")
    		} else {
    			$("#bg6_num" + i).removeClass("rel_number_copy").addClass("rel_number")
    			$("#bg6_jiantou" + i).removeClass("ft_copy").siblings().addClass("ft_copy")														
    			$("#bg6_ctxt" + i).removeClass("safe_wc_center_show")            
    		}
    	}
    	if(NowImg == MaxImg) 
    		NowImg = 1;
    	else
    		NowImg++; 
    }
</script>
