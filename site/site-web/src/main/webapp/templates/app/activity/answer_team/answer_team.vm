                        <p>当前团员 （$!{result.teamList.get(0).currentNumber}/$!{result.teamList.get(0).fullNumber}）</p>
                        <div class="team_card_ticket">
                            <div class="card_ticket_left ticket_left_color">
                                <span>3</span>元
                            </div>
                            <div class="card_ticket_right">
                                <p>投资金额5千元及以上</p>
                                <p>期限30天及以上</p>
                            </div>
                            <span class="card_ticket_dot dot_top"></span>
                            <span class="card_ticket_dot dot_bottom"></span>
                        </div>
                        <div class="team_card_btn">
                            #if($!{result.teamList.get(0).isStart} == "not_start")
                                <a>活动未开始</a>
                            #elseif($!{result.teamList.get(0).isStart} == "end")
                                <a>活动已结束</a>
                            #else
                                #if($!{result.isLogin} == "yes")
                                    #if($!{result.teamList.get(0).status} == "FULL")
                                        <a>抱团次数已满</a>
                                    #else
                                        #if($!{result.teamList.get(0).isJoined} == "YES")
                                            <a>已加入该团</a>
                                        #else
                                            <a class="gradient join_team" team_id="$!result.teamList.get(0).teamId" serial="$!result.teamList.get(0).serial">加入抱团</a>
                                        #end
                                    #end
                                #else
                                    <a class="gradient join_team" team_id="$!result.teamList.get(0).teamId" serial="$!result.teamList.get(0).serial">加入抱团</a>
                                #end
