                            #else
                                #if($!{result.isLogin} == "yes")
                                    #if($!{result.teamList.get(1).status} == "FULL")
                                        <a>抱团次数已满</a>
                                    #else
                                        #if($!{result.teamList.get(1).isJoined} == "YES")
                                            <a>已加入该团</a>
                                        #else
                                            <a class="gradient join_team" team_id="$!result.teamList.get(1).teamId" serial="$!result.teamList.get(1).serial">加入抱团</a>
                                        #end
                                    #end
                                #else
                                    <a class="gradient join_team" team_id="$!result.teamList.get(1).teamId" serial="$!result.teamList.get(1).serial">加入抱团</a>
                                #end
                            #end
                        </div>
                    </div>

                    <div class="team_card_bottom_sm hidden_lg">
                        <div class="sm_card_ticket">
                            <div class="sm_ticket_left">
                                <p><span>￥</span>8</p>
