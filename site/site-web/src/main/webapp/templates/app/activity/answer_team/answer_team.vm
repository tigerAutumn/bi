                            #else
                            <div class="team_card">
                            #end
                        #else
                        <div class="team_card">
                        #end
                    #else
                    <div class="team_card allowed" team_id="$!result.teamList.get(1).teamId" serial="$!result.teamList.get(1).serial">
                    #end
