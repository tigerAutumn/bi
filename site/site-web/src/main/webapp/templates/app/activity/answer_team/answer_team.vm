                                #elseif($!{result.teamList.get(0).isStart} == "end")
                                    <a class="sm_ticket_btn">已<br>结<br>束</a>
                                #else
                                    #if($!{result.isLogin} == "yes")
                                        #if($!{result.teamList.get(0).status} == "FULL")
                                            <a class="sm_ticket_btn">次<br>数<br>已<br>满</a>
                                        #else
                                            #if($!{result.teamList.get(0).isJoined} == "YES")
