                                            #else
                                                <a class="sm_ticket_btn join_team" team_id="$!result.teamList.get(0).teamId" serial="$!result.teamList.get(0).serial">加<br>入<br>抱<br>团</a>
                                            #end
                                        #end
                                    #else
                                        <a class="sm_ticket_btn join_team" team_id="$!result.teamList.get(0).teamId" serial="$!result.teamList.get(0).serial">加<br>入<br>抱<br>团</a>
                                    #end
                                #end
                            </div>
                            <span class="card_ticket_dot dot_top"></span>
                            <span class="card_ticket_dot dot_bottom"></span>
                        </div>
                    </div>
                </div>

                #if($!{result.teamList.get(1).isStart} == "start")
                    #if($!{result.isLogin} == "yes")
                        #if($!{result.teamList.get(1).status} != "FULL")
                            #if($!{result.teamList.get(1).isJoined} != "YES")
