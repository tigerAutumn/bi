#Html("begin")
#GenHead("begin")
###Title("【币港湾官网】国资背景互联网金融服务平台")
#LoadGenCSS("/resources/gen178/styles/agree/agree-print")
#GenHead("end")
	
<body>
	 <div class="">
  		<div class="agess" id="agree_content">
			<!--startprint1-->
  			<div class="agess_title">
  				<span class="agess_ft">出借咨询与服务协议</span>
  				<i class="agess_off close"></i>
  			</div>
  			<div  class="agess_main">
  				<h1 class="main_title">币港湾投资服务协议</h1>
  				<div class="serial">	
  					<p class="serial_main">当前用户不存在该加入记录</p>
  				</div>
				
			</div>
			<!--endprint1-->
  			<div class="agess_foot">
				<div class="agess_btn" onclick="oprin()">打印协议</div>
  				<div class="agess_btn close">关闭窗口</div>
  			</div>
  		</div>
  </div>  
#LoadGenJS("/resources/gen178/scripts/common/jquery")
<script>
$(function(){
	var close=$(".close");
	
	close.on("click",off);
	function off(){
		window.close();
	}
});

function oprin(){
	$(".agess_foot").hide();
	setTimeout(function(){$(".agess_foot").show();},600);
	window.print();
}


</script>
</body>
#Html("end")